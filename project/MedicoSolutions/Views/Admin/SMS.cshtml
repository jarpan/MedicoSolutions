@{
    ViewBag.Title = "SMS";
}

<script src="../../Scripts/jquery-1.4.4.min.js" type="text/javascript"></script>
<script type="text/javascript">
 
   

    function sms1() {
        //debugger;
        var name = $('#text1').val();
        var phno = $('#text2').val();
        
        $.ajax({
            type: "Get",
            data: { name: name, phno: phno},
            url: "/Admin/sms2",
            contentType: "application/json;charset=utf-8",
            dataType: "json",
            success: function (response) {
                alert(response);
            }
        });

    }

    </script>
<input id="Text1" type="text" />
<input id="Text2" type="text" />
<input id="Submit1" type="submit" value="submit" onclick="sms1();" />